<!DOCTYPE html>
@model AdminPortalModels.ViewModels.Application
<style>
    html, body {
        height: 100%;
    }

    table {
        border-collapse: collapse;
    }

    .border_display {
        border-style: solid;
        border-color: lightgray;
        border-width: 1px;
        border-collapse: collapse;
    }

    table.table-td-border td {
        border-style: solid;
        border-color: gray;
        border-width: 1px;
        height: 45px;
    }

    .td-settings {
        text-align: center;
        width: 17px;
        height: 20px;
    }

    .income-type-header {
        width: 25px;
        height: 15px;
        padding: 0px;
        font-size: 7px;
        font-family:Arial;
    }

    .height-20 {
        height: 20px;
    }

    /*Font Sizes*/
    .font-15 {
        font-size: 15px;
    }
     .font-14 {
        font-size: 14px;
    }
     .font-13 {
        font-size: 13px;
    }

    .font-12 {
        font-size: 12px;
    }

    .font-10 {
        font-size: 10px;
    }

    .font-9 {
        font-size: 9px;
    }

    .font-8 {
        font-size: 8px;
    }

    .font-7 {
        font-size: 7px;
    }

    .font-6 {
        font-size: 6px;
    }
    /*Font Weights*/
    .font-bold {
        font-weight: bold;
    }

    /*margins*/
    .no-margin {
        margin: 0px;
    }

    .all-margin-5 {
        margin: 5px;
    }

    .top-margin-7 {
        margin-top: 7px;
    }

    .top-margin-10 {
        margin-top: 10px;
    }

    .top-margin-15 {
        margin-top: 15px;
    }

    .top-margin-20 {
        margin-top: 20px;
    }

    .left-margin-5 {
        margin-left: 5px;
    }

    .left-margin-10 {
        margin-left: 10px;
    }

    .left-margin-15 {
        margin-left: 15px;
    }

    /*paddings*/
    .all-padding-3 {
        padding: 3px;
    }

    /*Step Header*/
    div .step-header {
        margin-top: 8px;
        background-color: #33AE6F;
        color: white;
        white-space: nowrap;
    }

        div .step-header table {
            height: 27px;
        }

        div .step-header td:first-child {
            background-color: #1F823F;
            font-size: 13px;
            width: 65px;
        }

        div .step-header td:last-child {
            font-size: 10px;
            font-weight: bold;
        }

        div .step-header strong, div .step-header span {
            margin-left: 5px;
            margin-right: 10px;
        }

    /*Float Left*/
    .float-left {
        float: left;
    }

    div .inner-divs-float-left div {
        float: left;
    }

    .display-flex {
        display: flex;
    }


    .definitation-box {
        border-style: solid;
        border-color: gray;
        border-width: 1px;
        float: left;
        margin-top: 15px;
        width: 110px;
    }

        .definitation-box p {
            font-size: 8px;
            margin: 5px 5px 0px 5px;
        }

    div .display-td {
        float: left;
        margin-top: 15px;
        margin-left: 15px;
    }

        div .display-td td {
            border-width: 1px;
            border-color: lightgray;
            border-style: solid;
            text-align: center;
            width: 17px;
            height: 20px;
        }

        div .display-td tr:nth-child(even) td {
            border-width: 0px;
            border-color: none;
            border-style: none;
            text-align: center;
            width: 17px;
            height: 3px;
        }

    .div-settings-1 {
        float: left;
        margin-top: 10px;
        margin-left: 15px;
        font-size: 8px;
    }

    .div-settings-2 {
        float: left;
        margin-top: 5px;
        margin-left: 5px;
        font-size: 7.49px;
        text-align: left;
    }

    div .display-tr tr {
        border-width: 1px;
        border-color: lightgray;
        border-style: solid;
        width: 100px;
        height: 21px;
    }

        div .display-tr tr:nth-child(even) {
            border-width: 0px;
            border-color: none;
            border-style: none;
            text-align: center;
            width: 21px;
            height: 3px;
        }

    .instructions-box {
        border-style: solid;
        border-width: 1px;
        border-color: gray;
        width: 220px;
        margin-top: 10px;
        float: left;
    }

        .instructions-box p {
            font-size: 8px;
            margin: 5px;
            word-wrap:break-word;
            vertical-align:middle;
            padding-top:50%;
        }

    .text-center {
        text-align: center;
    }

    .middle {
        vertical-align: middle;
    }

        .middle.step4 span {
            vertical-align: middle;
            margin-left: 5px;
        }

    div.instructions-step {
        font-size: 10px;
        overflow: auto;
        margin-top: 10px;
        /*page-break-before: always;*/
        width: 100%;
    }

        div .instructions-step .first-table {
            height: 200px;
        }

        div .instructions-step .second-table {
            height: 100%;
        }

        div.instructions-step table {
            width: 100%;
            word-wrap: break-word;
        }

        div .instructions-step td {
            border-style: solid;
            border-color: lightgray;
            border-width: 1px;
            height: 45px;
            padding-left: 5px;
        }

        div .instructions-step tr:first-child td {
            font-size: 12px;
            text-align: center;
            height: auto;
        }

        div .instructions-step .first-table tr:nth-child(2) td {
            text-align: center;
            width: 50%;
            height: 25px;
        }

        div .instructions-step .second-table tr:nth-child(2) td {
            text-align: center;
            width: 33%;
            height: auto;
        }

        div .instructions-step .second-table tr:nth-child(3) td {
            width: 33%;
            height: 195px;
        }

    .vertical-line {
        float:left;
        border-left-color: black;
        border-left-style: solid;
        border-left-width: 1px;
        border-collapse: collapse;
        margin-top: 30px;
        margin: 30px 5px 10px 1px;
    }

    .vertical-text {
        float:left;
        padding-top:11%;
        
    }

    .vertical-text span{
        letter-spacing: 0.1em;
        /*float: left;                 /* NOOOOOOOOOOOO :) */
        display: inline-block; /* use this instead of float:left */
        transform: rotate(-90deg) translateY(50%); /* Add: translateY 50% */
        width: 3em; /* same as font size (OR A BIT SMALLER) */
        text-align: center; /* to center text inside the red thing */
        vertical-align:top; /* top to "center" span... (yeah I know...) */
        /*background: rgba(255,0,0,0.1); just to see what the heck we're doing */
        white-space: nowrap; /* prevent small text wrap at 8em */
    }

    /* Hexagone*/
    .box {
  width: 120px;
  background-color: transparent;  
  margin-bottom: 20px;
  padding: 10px;
  position: relative;
  border:1px solid black;
}
.box:before, .box:after {
  width: 20px;
  height: 50%;
  position: absolute;
  left: 100%;
  content: "";
}
.box:before {
  top: 0px;
  background: linear-gradient(to right top, black 50%, rgba(0, 0, 0, 0) 50%);
}
.box:after {
  top: 50%;
  background: linear-gradient(to right bottom, #307084 50%, rgba(0, 0, 0, 0) 50%);
}

.page-break {
    page-break-before: always;
}
</style>
<div class="font-10" style="font-family:Arial; margin-top:10px;">
<div>
    <label style="font-family:'Arial Black'" class="font-14">Household Application for Free and Reduced Price School Meals</label>
    <label class="font-14" style="margin-left:120px;">Apply online:</label><a class="font-10" style="margin-left:5px;">www.myschoolaccount.com</a>
</div><div class="font-12">Complete one application per household. Please use a pen (not a pencil).</div>
<div id="Step_1_header" class="step-header">
    <table cellpadding="0" cellspacing="0">
        <tbody>
        <tr>
            <td>
                <strong>STEP 1</strong>
            </td>
            <td>
                <span>List ALL Household Members who are infants, children, and students up to and including grade 12 (if more spaces are required for additional names, attach another sheet of paper)</span>
            </td>
        </tr>
        </tbody>
    </table>
</div>
<div id="Step_1_Detail" style="overflow:auto;">
<div class="float-left display-flex">
<div class="definitation-box">
    <p>
        Definition of <b>
            Household
            Member
        </b>: "Anyone who is
        living with you and shares
        income and expenses, even
        if not related."<br /><br />
        Children in <b>Foster care</b> and
        children who meet the
        definition of <b>Homeless</b>,
        <b>Migrant</b> or <b>Runaway</b> are
        eligible for free meals. Read
        <b>
            How to Apply for Free and
            Reduced Price School
            Meals
        </b> for more information.
    </p>
</div>
<div id="Child_First_Name" class="display-td">
    <strong>Child’s First Name</strong>
    <div style="margin-top:5px;">
        <table cellpadding="0" cellspacing="0">
            @foreach (var stud in Model.Students)
            {
                <tr>
                    @for (int j = 0; j < 15; j++)
                    {
                        <td>
                            @if (@stud.First_Name.Length > j)
                            {
                                @stud.First_Name[j].ToString().ToUpper();
                            }
                        </td>
                    }
                </tr>
                <tr>
                    <td></td>
                </tr>
            }
            @foreach (var stud in Model.StudentsNotEnrolled)
            {
                <tr>
                    @for (int j = 0; j < 15; j++)
                    {
                        <td>
                            @if (@stud.First_Name.Length > j)
                            {
                                @stud.First_Name[j].ToString().ToUpper();
                            }
                        </td>
                    }
                </tr>
                <tr>
                    <td></td>
                </tr>
            }
            @if (Model.Students.Count() + Model.StudentsNotEnrolled.Count() < 5)
            {
                for (int i = (Model.Students.Count() + Model.StudentsNotEnrolled.Count()); i < 5; i++)
                {
                    <tr>
                        @for (int j = 0; j < 15; j++)
                        {
                            <td></td>
                        }
                    </tr>
                    <tr>
                        <td></td>
                    </tr>
                }
            }
        </table>
    </div>
</div>

