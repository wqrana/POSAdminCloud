<div id="Is_Student" class="div-settings-1">
    <div style="text-align:center;">
        Students?
    </div>
    <table>
        <tr>
            <td>Yes</td>
            <td width="25px;"></td>
            <td>No</td>
        </tr>
    </table>
    <div class="display-tr" style="margin:0px;">
        <table cellpadding="0" cellspacing="0">
            @foreach (var stud in Model.Students)
            {
                <tr>
                    <td>
                        <input type="checkbox" checked="checked" style="margin:3px 0px 3px 5px;" />
                    </td>
                    <td>
                        <input type="checkbox" style="margin:3px 5px 3px 20px;" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <div style="height:3px;"></div>
                    </td>
                </tr>
            }
            @foreach (var stud in Model.StudentsNotEnrolled)
            {
                <tr>
                    <td>
                        <input type="checkbox" style="margin:3px 0px 3px 5px;" />
                    </td>
                    <td style="margin-left:10px;">
                        <input type="checkbox" checked="checked" style="margin:3px 5px 3px 20px;" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <div style="height:3px;"></div>
                    </td>
                </tr>
            }
            @if (Model.Students.Count() + Model.StudentsNotEnrolled.Count() < 5)
            {
                for (int i = (Model.Students.Count() + Model.StudentsNotEnrolled.Count()); i < 5; i++)
                {
                    <tr>
                        <td>
                            <input type="checkbox" style="margin:3px 0px 3px 5px;" />
                        </td>
                        <td style="margin-left:10px;">
                            <input type="checkbox" style="margin:3px 5px 3px 20px;" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div style="height:3px;"></div>
                        </td>
                    </tr>
                }
            }
        </table>
    </div>
</div>
<div class="vertical-text">
    <span>
        Check all that apply
    </span>
</div>
<div class="vertical-line">

</div>
<div id="Child_Special-Circumstances" class="div-settings-2" style="font-family:sans-serif 'Times New Roman', Times, serif;">
    <table>
        <tr>
            <td style="vertical-align:bottom;">Foster<br />Child</td>
            <td width="5px;"></td>
            <td style="vertical-align:top;">Homeless,<br />Migrant,<br />Runaway</td>
        </tr>
    </table>
    <div class="display-tr" style="margin-top:0px; padding:0px;">
        <table cellpadding="0" cellspacing="0">
            @foreach (var stud in Model.Students)
            {
                <tr>
                    <td>
                        <input type="checkbox" checked="@stud.Foster" style="margin:3px 0px 3px 5px;" />
                    </td>
                    <td>
                        <input type="checkbox" checked="@(!Model.SpecialCircum.Contains("Not provided"))" style="margin:3px 5px 3px 20px;" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <div style="height:3px;"></div>
                    </td>
                </tr>
            }
            @foreach (var stud in Model.StudentsNotEnrolled)
            {
                <tr>
                    <td>
                        <input type="checkbox" checked="@stud.FosterChild" style="margin:3px 0px 3px 5px;" />
                    </td>
                    <td style="margin-left:10px;">
                        <input type="checkbox" checked="@(!Model.SpecialCircum.Contains("Not provided"))" style="margin:3px 5px 3px 20px;" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <div style="height:3px;"></div>
                    </td>
                </tr>
            }
            @if (Model.Students.Count() + Model.StudentsNotEnrolled.Count() < 5)
            {
                for (int i = (Model.Students.Count() + Model.StudentsNotEnrolled.Count()); i < 5; i++)
                {
